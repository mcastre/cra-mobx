{
  "name": "cra-mobx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "npm run build -- --stats && npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "build": "node scripts/build.js",
    "dev": "npm-run-all --parallel watch-server start",
    "lint": "eslint --fix ./src -c .eslintrc.js --ext .js",
    "server": "node scripts/server.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js",
    "watch-server": "nodemon scripts/server.js"
  },
  "dependencies": {
    "@babel/polyfill": "~7.0.0",
    "@reach/router": "~1.2.1",
    "axios": "~0.18.0",
    "custom-event-polyfill": "~1.0.7",
    "emotion": "~9.2.12",
    "emotion-theming": "~9.2.9",
    "focus-trap-react": "~6.0.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "mobx": "^4.11.0",
    "mobx-react": "^6.1.1",
    "moment": "~2.29.2",
    "prop-types": "~15.6.2",
    "query-string": "~5.1.1",
    "react": "~16.8.6",
    "react-app-polyfill": "~0.1.3",
    "react-dom": "~16.8.6",
    "react-emotion": "~9.2.12",
    "react-idle-timer": "~4.2.3",
    "react-redux": "~5.1.1",
    "redux": "~4.0.1",
    "redux-saga": "~1.0.2",
    "redux-select": "~3.5.2",
    "validator": "~10.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "@testing-library/react": "^8.0.4",
    "@welldone-software/why-did-you-render": "^3.0.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-mobx": "^2.0.0",
    "babel-preset-react-app": "^5.0.3",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "chance": "^1.0.18",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "css-loader": "1.0.0",
    "debug": "^4.1.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "file-loader": "2.0.0",
    "flow-bin": "^0.100.0",
    "flow-typed": "^2.5.2",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "http-errors": "^1.7.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^23.6.0",
    "jest-bamboo-reporter": "^1.2.1",
    "jest-dom": "^3.5.0",
    "jest-emotion": "^9.2.11",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jest-snapshot": "^24.3.1",
    "lint-staged": "^8.1.5",
    "majestic": "^1.2.23",
    "mini-css-extract-plugin": "0.4.3",
    "mobx-logger": "^0.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "react-dev-utils": "^6.0.4",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.3",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/serviceWorker.js",
      "!src/index.js"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/test-report.json"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^lodash-es$": "lodash"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "jest-emotion/serializer"
    ],
    "testResultsProcessor": "jest-bamboo-reporter"
  },
  "babel": {
    "presets": [
      "react-app",
      "mobx"
    ],
    "plugins": [
      [
        "emotion",
        {
          "sourceMap": true
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
